{
    "name": "lin3s/distribution",
    "description": "Distribution library for different purposes inside LIN3S",
    "keywords": ["lin3s", "distribution"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LIN3S",
            "email": "info@lin3s.com",
            "homepage": "https://lin3s.com"
        }
    ],
    "require": {
        "symfony/filesystem": "^2.3 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.12"
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix --config-file=.php_cs"
        ]
    },
    "autoload": {
        "psr-4": {
            "LIN3S\\Distribution\\": "src/"
        },
        "exclude-from-classmap": [
            "/Javascript/"
        ]
    }
}
